        <script>
                    $(document).ready(function() {
                                            $('#slider-block').flexslider({
                                                                        animation : "slide",
                                                                                            slideshow : true,
                                                                                                                pauseOnHover : true,
                                                                                                                                    autoPlay : true,
                                                                                                                                                        controlNav : true,
                                                                                                                                                                            animationLoop : true,
                                                                                                                                                                                                start : function(slider) {
                                                                                                                                                                                                                        slider.find('.slideIndex').html(slider.currentSlide + 1 + "/" + slider.count);
                                                                                                                                                                                                                                            },
                                                                                                                                                                                                                                                                after : function(slider) {
                                                                                                                                                                                                                                                                                        slider.find('.slideIndex').html(slider.currentSlide + 1 + "/" + slider.count);
                                                                                                                                                                                                                                                                                                                if (!slider.playing) {
                                                                                                                                                                                                                                                                                                                                            slider.play();
                                                                                                                                                                                                                                                                                                                                                                    }
                                                                                                                                                                                                                                                                                                                                                                                        }
                                                                                                                                                                                                                                                                                                                                                                                                        });

