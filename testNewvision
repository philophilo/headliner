        </script>
                <script>
                            $('.article-content').jfontsize({
                                                    btnMinusClasseId : '.jfont-small',
                                                                    btnDefaultClasseId : '.jfont-medium',
                                                                                    btnPlusClasseId : '.jfont-big',

                                                                                                });
            $(".jfontsize-button").click(function() {
                                    var a = $(this).attr("data_it");
                                                    $(".jfontsize-button").removeClass("active");
                                                                    $("." + a).addClass("active");
                                                                                });

        </script>
                <script>
                            $('.more_less').click(function() {
                                                    if ($(this).attr('data-type') == "More") {
                                                                        $(this).hide();
                                                                                            $(this).next().show();
                                                                                                                var t = $(this).parents('.search_result_content').find('ul>li').length;
                                                                                                                                    $(this).parents('.search_result_content').find('ul>li').slice(0, t).slideDown("fast");
                                                                                                                                                    } else {
                                                                                                                                                                        $(this).hide();
                                                                                                                                                                                            $(this).prev().show();
                                                                                                                                                                                                                var t = $(this).parents('.search_result_content').find('ul>li').length;
                                                                                                                                                                                                                                    $(this).parents('.search_result_content').find('ul>li').show().slice(3, t).slideUp("fast");
                                                                                                                                                                                                                                                    }
                                                                                                                                                                                                                                                                });

        </script>
