                <script>
                            $(document).ready(function() {
                                                    $(".col-list .common-left").click(function(e) {
                                                                                e.stopPropagation();
                                                                                                    $(".list_discription").show();
                                                                                                                        $(".list_discription").show();
                                                                                                                                            $(".active_list").hide();
                                                                                                                                                                $(".list_discription", this).hide();
                                                                                                                                                                                    $(".active_list", this).show();
                                                                                                                                                                                                    });
                                                                });

        </script>
                
