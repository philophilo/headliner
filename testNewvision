            $('#carousel_two_slider1 li').click(function(event) {
                                    event.preventDefault();
                                                    $('#carousel_two_slider1 li').removeClass("flex-active-slide");
                                                                    $('#col_two_slider1').flexslider(Number($(this).index()));
                                                                                    $(this).addClass("flex-active-slide");
