<script>
$(document).ready(function() {
            $('.video-source li img').on('click', function(event) {
                         event.preventDefault();
                                 var source=$(this).attr("data-src");
                                         $('#slider .video-content').hide();
                                                 $('#carousel li img').removeClass('select');
                                                         $(this).toggleClass("select");
                                                               
                                                                 new_source = source + "?autoplay=1";
                                                                          console.log(source);
                                                                                    $("#slider .video-block iframe").attr("src",new_source);
                                                                                              $("html, body").animate({ scrollTop: 200 }, 600);

                                                                                                        
                                                                                                    var title=$('#carousel li img.select').attr('data-title');
                                                                                                            var total="#"+title;
                                                                                                                    $(total).show();


                                                                                                                             });
                     });        
</script>
