                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         var elem = document.getElementById('mySwipe');
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             number_of_photos = document.getElementById('tocount').getElementsByTagName('div').length;
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 for (var i = 0; i < number_of_photos; i++) {
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             var counter = i+1;
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     var div = document.getElementById('position');
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             div.innerHTML = div.innerHTML + '<a onclick="mySwipe.slide(' + (counter-1) + ');return false;">' + '<em>' + counter + '</em></a>';
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 }
    bullets = document.getElementById('position').getElementsByTagName('em');
        bullets[0].className = "on";

            window.mySwipe = Swipe(elem, {
                            callback: function(e, pos) {
                                        pos = mySwipe.getPos();
                                                    var i = bullets.length;
                                                                while (i--) {
                                                                                bullets[i].className = ' ';
                                                                                            }
                                                                                                        bullets[pos].className = "on";
                                                                                                                },
                                                                                                                    });
    function leftArrowPressed() {
                mySwipe.prev();
                    }

    function rightArrowPressed() {
                mySwipe.next();
                    }

    document.onkeydown = function(evt) {
                var element = document.activeElement.tagName;
                        console.log(element);
                                if (element != 'INPUT' || element != 'TEXTFIELD') {
                                                evt = evt || window.event;
                                                            switch (evt.keyCode) {
                                                                                case 37:
                                                                                                        leftArrowPressed();
                                                                                                                            break;
                                                                                                                                            case 39:
                                                                                                                                                rightArrowPressed();
                                                                                                                                                                    break;
                                                                                                                                                                                }
                                                                    }
                                    };
